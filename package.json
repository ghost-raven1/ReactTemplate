{
  "name": "my-antd-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:dev": "cypress open",
    "analyze": "vite build --mode analyze",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "deploy:staging": "pnpm run build && node scripts/deploy.js staging",
    "deploy:prod": "pnpm run build && node scripts/deploy.js production"
  },
  "dependencies": {
    "@ant-design/icons": "6.0.0",
    "@centrifuge/centrifuge-js": "0.9.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.0",
    "@fontsource/montserrat": "5.0.8",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^9.0.8",
    "@unleash/proxy-client-react": "5.0.0",
    "antd": "5.15.1",
    "axios": "^1.6.7",
    "centrifuge": "5.3.5",
    "dayjs": "1.11.10",
    "framer-motion": "11.0.8",
    "i18next": "23.10.1",
    "i18next-browser-languagedetector": "7.2.0",
    "react": "18.2.0",
    "react-cookie": "6.1.1",
    "react-dev-inspector": "2.0.1",
    "react-dom": "18.2.0",
    "react-error-boundary": "6.0.0",
    "react-hotkeys-hook": "4.4.1",
    "react-i18next": "14.0.5",
    "react-loading-skeleton": "3.4.0",
    "react-router-dom": "^6.22.3",
    "styled-components": "^6.1.8",
    "unleash-client": "6.6.0",
    "unleash-proxy-client": "3.7.6",
    "uuid": "^9.0.1",
    "vite-plugin-compression2": "1.4.0",
    "vite-plugin-imagemin": "0.6.1",
    "yup": "1.3.3",
    "zod": "3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@eslint/js": "^8.57.0",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "cypress": "^13.6.6",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^14.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "rollup-plugin-visualizer": "5.12.0",
    "terser": "5.29.2",
    "typedoc": "0.25.8",
    "typescript": "~5.4.2",
    "typescript-eslint": "^7.1.0",
    "vite": "6.3.4",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.19.0",
    "vitest": "3.0.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.18.2",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.4"
}
