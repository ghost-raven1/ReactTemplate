# Save exact version of packages
save-exact=true

# Use package-lock.json
package-lock=true

# Use npm audit
audit=true

# Use npm fund
fund=false

# Use npm update
update-notifier=true

# Use npm cache
cache-min=3600

# Use npm registry
registry=https://registry.npmjs.org/

# Use npm scripts
ignore-scripts=false

# Use npm engine
engine-strict=true

# Use npm progress
progress=true

# Use npm loglevel
loglevel=error 